{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single",
            "avoid-escape",
            "avoid-template"
        ],
        "semicolon": [
            true,
            "always",
            "strict-bound-class-methods"
        ],
        "interface-name": [
            false
        ]
    },
    "rulesDirectory": []
}